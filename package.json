{
  "name": "transmax",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "7.9.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "pg": "^7.4.3",
